{
    "version": 2,
    "builds": [
        {
            "src": "controller/*.py",
            "use": "@vercel/python",
            "config": {
                "runtime": "python3.12"
            }
        },
        {
            "src": "./index.py",
            "use": "@vercel/python"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/index.py"
        }
    ]
}